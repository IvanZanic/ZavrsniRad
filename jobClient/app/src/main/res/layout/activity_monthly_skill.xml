<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:orientation="vertical"
    android:gravity="center|top"
    tools:context="com.moje.jobclient.app.MonthlySkillActivity">

    <Button
        android:id="@+id/myDatePickerButton1"
        android:text="Početni datum"
        android:textColor="#FFFFFF"
        android:textSize="15sp"
        android:layout_marginTop="25dp"
        android:layout_width="200dp"
        android:layout_height="35dp"
        android:background="@drawable/datebuttonshape"
        android:shadowColor="#A8A8A8"
        android:shadowDx="0"
        android:shadowDy="0"
        android:shadowRadius="5"
        />

    <TextView android:id="@+id/showMyDate1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="25dp"
        android:layout_marginTop="15dp"/>

    <Button
        android:id="@+id/myDatePickerButton2"
        android:text="Krajnji datum"
        android:layout_marginTop="40dp"
        android:textColor="#FFFFFF"
        android:textSize="15sp"
        android:layout_width="200dp"
        android:layout_height="35dp"
        android:background="@drawable/datebuttonshape"
        android:shadowColor="#A8A8A8"
        android:shadowDx="0"
        android:shadowDy="0"
        android:shadowRadius="5"
        />

    <TextView android:id="@+id/showMyDate2"
        android:layout_width="wrap_content"
        android:textSize="25dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"/>

    <graphics.MultiSpinner android:id="@+id/traziSpinner"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:scrollbarSize="30dp"
        android:entries="@array/tehnologije_array"
        android:layout_marginTop="40dp"/>
    
    <Spinner android:id="@+id/countySpinner"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:entries="@array/county_array"
        android:layout_marginTop="40dp"/>

    <View
        android:id="@+id/line"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginTop="30dp"
        android:background="@android:color/black" />

    <Button android:id="@+id/prikaziButton"
        android:layout_width="200dp"
        android:layout_height="35dp"
        android:background="@drawable/buttonshape"
        android:textColor="#FFFFFF"
        android:text="Prikazi dijagram"
        android:layout_marginTop="30dp"
        android:textSize="15sp"
        android:shadowColor="#A8A8A8"
        android:shadowDx="0"
        android:shadowDy="0"
        android:shadowRadius="5" />

</LinearLayout>

